{
    "name"        : "src-run/arthur-doctrine-traits-library",
    "license"     : "MIT",
    "type"        : "library",
    "homepage"    : "https://src.run/arthur-doctrine-traits-library",
    "description" : "a collections of traits for common entity properties",
    "support" : {
        "email"  : "rmf@src.run",
        "issues" : "https://src.run/arthur-doctrine-traits-library/issues",
        "source" : "https://src.run/arthur-doctrine-traits-library/git"
    },
    "keywords"   : [
        "arthur-doctrine-traits-library", "arthur", "doctrine", "traits", "library", "rmf", "src.run"
    ],
    "authors"    : [
        {
            "name"    : "Rob Frawley 2nd",
            "email"   : "rmf@src.run",
            "homepage": "https://src.run/rmf",
            "role"    : "Project Lead"
        }
    ],
    "require" : {
        "php"                                       : "^5.6||^7.0",
        "src-run/arthur-doctrine-identity-library"  : "^0.6",
        "src-run/arthur-doctrine-exception-library" : "^0.6",
        "ramsey/uuid"                               : "^3.0"
    },
    "require-dev" : {
        "src-run/augustus-reflection-library" : "^0.6",
        "doctrine/orm"                        : "^2.5",
        "phpunit/phpunit"                     : "^5.0",
        "codacy/coverage"                     : "^1.0",
        "satooshi/php-coveralls"              : "^1.0"
    },
    "suggest" : {
        "doctrine/orm" : "^2.5"
    },
    "autoload" : {
        "psr-4" : {
            "SR\\Doctrine\\ORM\\Model\\" : "lib/"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "SR\\Doctrine\\ORM\\Model\\Tests\\" : "tests/"
        }
    },
    "minimum-stability" : "dev",
    "prefer-stable"     : true,
    "config": {
        "bin-dir" : "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master" : "0.6-dev"
        }
    }
}
