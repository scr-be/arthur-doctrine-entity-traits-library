{
    "name"        : "scr-be/arthur-doctrine-entity-traits-library",
    "license"     : "MIT",
    "type"        : "library",
    "homepage"    : "https://scr.be/arthur-doctrine-entity-traits-library",
    "description" : "This library contains a collections of traits for common entity property requirements.",
    "support" : {
        "email"  : "rmf@scr.be",
        "issues" : "https://arthur-doctrine-entity-traits-library.oss.scr.be/issues",
        "source" : "https://arthur-doctrine-entity-traits-library.oss.scr.be/git",
        "irc"    : "irc://irc.efnet.org/scribe"
    },
    "keywords"   : [
        "arthur-doctrine-entity-traits-library", "doctrine", "utilities", "library", "rmf", "scr.be", "scribe"
    ],
    "authors"    : [
        {
            "name"    : "Rob Frawley 2nd",
            "email"   : "rmf@scr.be",
            "homepage": "https://scr.be/rmf",
            "role"    : "Project Lead"
        }
    ],
    "require" : {
        "php"                            : "~5.6|~7.0",
        "scr-be/arthur-doctrine-library" : "~0.5"
    },
    "require-dev" : {
        "scr-be/arthur-doctrine-behavior-bundle" : "~0.1",
        "phpunit/phpunit"                        : "~5.0",
        "codacy/coverage"                        : "~1.0",
        "satooshi/php-coveralls"                 : "~1.0"
    },
    "suggest" : {
        "scr-be/arthur-doctrine-behavior-bundle" : "~0.1",
        "doctrine/orm"                           : "~2.5"
    },
    "autoload" : {
        "psr-4" : {
            "Scribe\\Doctrine\\ORM\\Model\\" : "lib/"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "Scribe\\Doctrine\\ORM\\Model\\Tests\\" : "tests/"
        }
    },
    "minimum-stability" : "dev",
    "prefer-stable"     : true,
    "config": {
        "bin-dir" : "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master" : "0.1-dev"
        }
    }
}
