{
    "name"        : "src-run/arthur-doctrine-entity-traits-library",
    "license"     : "MIT",
    "type"        : "library",
    "homepage"    : "https://get.src.run/arthur-doctrine-entity-traits-library",
    "description" : "This library contains a collections of traits for common entity property requirements.",
    "support" : {
        "email"  : "rmf@src.run",
        "issues" : "https://arthur-doctrine-entity-traits-library.oss.get.src.run/issues",
        "source" : "https://arthur-doctrine-entity-traits-library.oss.get.src.run/git",
        "irc"    : "irc://irc.efnet.org/scribe"
    },
    "keywords"   : [
        "arthur-doctrine-entity-traits-library", "doctrine", "utilities", "library", "rmf", "get.src.run", "scribe"
    ],
    "authors"    : [
        {
            "name"    : "Rob Frawley 2nd",
            "email"   : "rmf@src.run",
            "homepage": "https://get.src.run/rmf",
            "role"    : "Project Lead"
        }
    ],
    "require" : {
        "php"                             : "~5.6|~7.0",
        "src-run/arthur-doctrine-library" : "~0.5"
    },
    "require-dev" : {
        "phpunit/phpunit"        : "~5.0",
        "codacy/coverage"        : "~1.0",
        "satooshi/php-coveralls" : "~1.0"
    },
    "suggest" : {
        "src-run/arthur-doctrine-behavior-bundle" : "~0.5",
        "doctrine/orm"                            : "~2.5"
    },
    "autoload" : {
        "psr-4" : {
            "SR\\Doctrine\\ORM\\Model\\" : "lib/"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "SR\\Doctrine\\ORM\\Model\\Tests\\" : "tests/"
        }
    },
    "minimum-stability" : "dev",
    "prefer-stable"     : true,
    "config": {
        "bin-dir" : "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master" : "0.5-dev"
        }
    }
}
